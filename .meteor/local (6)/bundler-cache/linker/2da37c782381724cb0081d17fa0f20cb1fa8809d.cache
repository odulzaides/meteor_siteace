                Session.set(\"websiteLimit\", Session.get(\"websiteLimit\") + 4);\n            }                                                          //\n                                                                       //\n            lastScrollTop = scrollTop;                                 // 22\n        }                                                              //\n    });                                                                //\n                                                                       //\n    Accounts.ui.config({                                               // 27\n        passwordSignupFields: \"USERNAME_AND_EMAIL\"                     // 28\n    });                                                                //\n                                                                       //\n    /////                                                              //\n    // template helpers                                                //\n    /////                                                              //\n                                                                       //\n    // helper function that returns all available websites             //\n    Template.website_list.helpers({                                    // 38\n        websites: function () {                                        // 39\n            return Websites.find({});                                  // 40\n        }                                                              //\n                                                                       //\n    });                                                                //\n                                                                       //\n    Template.body.helpers({                                            // 45\n        username: function () {                                        // 46\n            if (Meteor.user()) {                                       // 47\n                return Meteor.user().username;                         // 48\n                //return Meteor.user().emails[0].address;              //\n            } else {                                                   //\n                    return \"anonymous internet user\";                  // 53\n                }                                                      //\n        }                                                              //\n    });                                                                //\n    Template.website_item.helpers({                                    // 57\n        getUser: function (user_id) {                                  // 58\n            var user = Meteor.users.findOne({ _id: user_id });         // 59\n            console.log(user);                                         // 60\n            if (user) {                                                // 61\n                return user.username;                                  // 62\n            } else {                                                   //\n                return \"Anonymous internet user\";                      // 64\n            }                                                          //\n        },                                                             //\n        setDate: function (createdOn) {                                // 67\n            var date = new Date();                                     // 68\n            var createDate = moment(date).format(\"MM/DD/YY\");          // 69\n            return createDate;                                         // 70\n        },                                                             //\n        setTime: function (createdOn) {                                // 72\n            var createTime = moment(date).format(\"h:hh A\");            // 73\n            return createTime;                                         // 74\n        }                                                              //\n    });                                                                //\n                                                                       //\n    /////                                                              //\n    // template events                                                 //\n    /////                                                              //\n                                                                       //\n    Template.website_item.events({                                     // 84\n        \"click .js-upvote\": function (event) {                         // 85\n            // example of how you can access the id for the website in the database\n            // (this is the data context for the template)             //\n            var website_id = this._id;                                 // 88\n            console.log(\"Up voting website with id \" + website_id);    // 89\n            //                                                         //\n            // TODO - put the code in here to add a vote to a website!\n            Websites.update({ _id: website_id }, { $inc: { upvote: +1 } }); // for a vote UP\n                                                                       //\n            return false; // prevent the button from reloading the page\n        },                                                             //\n        \"click .js-downvote\": function (event) {                       // 96\n                                                                       //\n            // example of how you can access the id for the website in the database\n            // (this is the data context for the template)             //\n            var website_id = this._id;                                 // 100\n            console.log(\"Down voting website with id \" + website_id);  // 101\n                                                                       //\n            // TODO - put the code in here to remove a vote from a website!\n            Websites.update({ _id: website_id }, { $inc: { downvote: +1 } });\n                                                                       //\n            return false; // prevent the button from reloading the page\n        }                                                              //\n    });                                                                //\n                                                                       //\n    Template.website_form.events({                                     // 110\n        \"click .js-toggle-website-form\": function (event) {            // 111\n            $(\"#website_form\").toggle('slow');                         // 112\n        },                                                             //\n        \"submit .js-save-website-form\": function (event) {             // 114\n                                                                       //\n            // here is an example of how to get the url out of the form:\n            var url = event.target.url.value;                          // 117\n            var title = event.target.title.value;                      // 118\n            var description = event.target.description.value;          // 119\n            console.log(\"The url they entered is: \" + url);            // 120\n                                                                       //\n            //  TODO - put your website saving code in here!           //\n            if (Meteor.user()) {                                       // 123\n                var date = Date();                                     // 124\n                Websites.insert({                                      // 125\n                    url: url,                                          // 126\n                    title: title,                                      // 127\n                    description: description,                          // 128\n                    createdOn: date,                                   // 129\n                    createdBy: Meteor.user()._id,                      // 130\n                    upVote: 0,                                         // 131\n                    downVote: 0                                        // 132\n                });                                                    //\n                                                                       //\n                console.log(date);                                     // 135\n                $(\"#url\").val(\" \");                                    // 136\n                $(\"#title\").val(\" \");                                  // 137\n                $(\"#description\").val(\" \");                            // 138\n                $(\"#website_form\").toggle('slow');                     // 139\n                                                                       //\n                return false;                                          // 141\n            }                                                          //\n                                                                       //\n            return false; // stop the form submit from reloading the page\n        }                                                              //\n    });                                                                //\n}                                                                      //\nvar date = Date();                                                     // 150\n                                                                       //\nif (Meteor.isServer) {                                                 // 152\n    // start up function that creates entries in the Websites databases.\n    Meteor.startup(function () {                                       // 154\n        // code to run on server at startup                            //\n        if (!Websites.findOne()) {                                     // 156\n            console.log(\"No we